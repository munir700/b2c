<?xml version="1.0" encoding="utf-8"?>
<co.yap.widgets.loading.CircularProgressBar xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/circularProgressBar"
    android:layout_width="@dimen/_90sdp"
    android:layout_height="@dimen/_90sdp"
    android:layout_centerInParent="true"
    android:layout_gravity="center"
    app:cpb_background_progressbar_color="@color/grey"
    app:cpb_background_progressbar_width="4dp"
    app:cpb_indeterminate_mode="true"
    app:cpb_progress="50"
    app:cpb_progress_direction="to_right"
    app:cpb_progress_max="100"
    android:visibility="visible"
    app:cpb_progressbar_color="@color/colorPrimary"
    app:cpb_progressbar_width="4dp"
    app:cpb_round_border="false"
    app:cpb_start_angle="-90" />
