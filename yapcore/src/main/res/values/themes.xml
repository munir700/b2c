<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CoreAppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#5E35B1</item>
        <item name="colorPrimaryDark">#272262</item>
        <item name="colorAccent">#4d4d4d</item>

        <item name="colorPrimaryAttr">@color/success</item>
        <item name="colorPrimaryDarkAttr">@color/warning</item>
        <item name="colorPrimaryAccentAttr">@color/colorAccent</item>

        //

        <item name="colorPrimaryAccentWarmAttr">@color/colorAccent</item>
        <item name="colorPrimaryWarmAttr">@color/colorPrimaryWarm</item>
        <item name="colorPrimaryLightAttr">@color/colorPrimaryLight</item>
        <item name="colorPrimaryAltAttr">@color/colorPrimaryAlt</item>
        <item name="colorPrimarySoftAttr">@color/colorPrimarySoft</item>



    </style>

    <style name="AppThemeHouseHold" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#fc6253</item>
        <item name="colorPrimaryDark">@color/textDarkColor</item>
        <item name="colorAccent">#fc6253</item>

        <item name="colorPrimaryAttr">@color/red</item>
        <item name="colorPrimaryDarkAttr">@color/textDarkColor</item>
        <item name="colorPrimaryAccentAttr">@color/colorAccentHouseHold</item>


        <item name="colorPrimaryAccentWarmAttr">@color/colorAccent</item>
        <item name="colorPrimaryWarmAttr">@color/colorPrimaryWarm</item>
        <item name="colorPrimaryLightAttr">@color/colorPrimaryLight</item>
        <item name="colorPrimaryAltAttr">@color/colorPrimaryAlt</item>
        <item name="colorPrimarySoftAttr">@color/colorPrimarySoft</item>

    </style>

    <style name="CoreAppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="CoreAppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="CoreAppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


</resources>