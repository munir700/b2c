<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CoreAppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#5E35B1</item>
        <item name="colorPrimaryDark">#272262</item>
        <item name="colorAccent">#4d4d4d</item>

        <item name="colorPrimaryAttr">@color/colorPrimary</item>
        <item name="colorPrimaryDarkAttr">@color/colorPrimaryDark</item>
        <item name="colorPrimaryAccentAttr">@color/colorAccent</item>

        <item name="colorPrimaryAccentWarmAttr">@color/colorAccent</item>
        <item name="colorPrimaryWarmAttr">@color/colorPrimaryWarm</item>
        <item name="colorPrimaryLightAttr">@color/colorPrimaryLight</item>
        <item name="colorPrimaryAltAttr">@color/colorPrimaryAlt</item>
        <item name="colorPrimarySoftAttr">@color/colorPrimarySoft</item>
        <item name="colorPrimaryDisabledBtnAttr">@color/colorDisabledBtn</item>
        <item name="colorPressedBtnStateAttr">@color/colorPressedBtnState</item>
        <item name="colordisabledTick">@color/disabledTickColor</item>
        <item name="colorSeekBar">@color/seekbarProgress</item>

        <item name="primaryButtonTheme">@style/Widget.Button.Primary</item>

    </style>

    <style name="AppThemeHouseHold" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#fc6253</item>
        <item name="colorPrimaryDark">@color/textDarkColor</item>
        <item name="colorAccent">#fc6253</item>

        <item name="colorPrimaryAttr">@color/colorPrimaryHouseHold</item>
        <item name="colorPrimaryDarkAttr">@color/textDarkColor</item>
        <item name="colorPrimaryAccentAttr">@color/colorAccentHouseHold</item>


        <item name="colorPrimaryAccentWarmAttr">@color/colorAccentHouseHold</item>
        <item name="colorPrimaryWarmAttr">@color/colorPrimary</item>
        <item name="colorPrimaryLightAttr">@color/colorDisabledBtnHouseHold</item>
        <item name="colorPrimaryAltAttr">@color/colorDisabledBtnHouseHold</item>
        <item name="colorPrimarySoftAttr">@color/colorPrimarySoftHouseHold</item>
        <item name="colorPrimaryDisabledBtnAttr">@color/colorDisabledBtnHouseHold</item>
        <item name="colorPressedBtnStateAttr">@color/colorPressedBtnStateHouseHold</item>
        <item name="colordisabledTick">@color/colordisabledTickHouseHold</item>
        <item name="colorSeekBar">@color/seekbarProgresskHouseHold</item>

        <item name="primaryButtonTheme">@style/Widget.Button.Primary</item>

    </style>

    <style name="CoreAppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="CoreAppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="CoreAppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppThemeHouseHold.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


    <style name="Widget.Button.Primary" parent="Widget.AppCompat.Button">
        <item name="colorButtonNormal">?attr/colorPrimaryAttr</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="CardViewStyle" parent="Widget.MaterialComponents.CardView">
        <item name="android:background">?attr/colorPrimaryWarmAttr</item>
     </style>


</resources>