<?xml version="1.0" encoding="utf-8"?>

                                                        

    

                 
                            
                                                                                    
           

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" android:tag="layout/activity_rates_alerts_0" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">

        <include
            android:id="@+id/toolbar"
            layout="@layout/layout_rates_and_alerts_toolbar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
                                         />

        <fragment
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/rates_and_alerts_nav_host_fragment"
            android:name="androidx.navigation.fragment.NavHostFragment"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/toolbar"
            app:defaultNavHost="true"
            app:navGraph="@navigation/navigation_rates_and_alerts" />


    </androidx.constraintlayout.widget.ConstraintLayout>
         