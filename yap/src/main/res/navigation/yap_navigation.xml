        tools:layout="@layout/fragment_yap_dashboard_home">
        <action
            android:id="@+id/action_yapDashboardHome_to_yapStoreFragment"
            app:destination="@id/yapStoreFragment" />
    </fragment>
    <fragment
        android:id="@+id/yapStoreFragment"
        android:name="co.yap.modules.store.fragments.YapStoreFragment"
        android:label="YapStoreFragment"
        tools:layout="@layout/fragment_yap_store" />
